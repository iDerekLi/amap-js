{
  "name": "amap-js",
  "version": "2.5.0",
  "author": "Derek Li",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "build/amap-js.c.js",
  "module": "build/amap-js.esm.js",
  "types": "types/amap-js.d.ts",
  "files": [
    "build",
    "types",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -cw",
    "build": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf \"./{build,types}\"",
    "release": "sh ./scripts/release.sh",
    "lint": "eslint src --fix --ext .js",
    "test": "jest",
    "coverage": "jest --coverage",
    "docs:dev": "cd docs && yarn dev",
    "docs:build": "cd docs && yarn build",
    "docs:deploy": "sh ./scripts/deploy.sh"
  },
  "keywords": [
    "amap",
    "jsapi",
    "sdk",
    "loader",
    "map",
    "gis",
    "高德",
    "地图"
  ],
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "homepage": "https://amapjs.derekli.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iDerekLi/amap-js.git"
