{
  "name": "amap-js",
  "version": "2.3.0",
  "description": "AMap高德地图API加载器。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/amap-js.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "types",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "webpack --config ./webpack.dev.js --mode development",
    "build": "npm run build:lib && npm run build:umd",
    "build:umd": "webpack --config ./webpack.config.js --mode production",
    "build:lib": "node ./build-lib.js",
    "release": "sh build/release.sh",
    "deploy": "sh build/deploy.sh",
    "lint": "eslint src --fix --ext .js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "amap",
    "jsapi",
    "sdk",
    "loader",
    "map",
    "gis",
    "高德",
    "地图"
  ],
  "author": "Derek Li",
  "homepage": "https://iderekli.github.io/amap-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iDerekLi/amap-js.git"
  },
  "bugs": {
    "url": "https://github.com/iDerekLi/amap-js/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.9.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  }
}
